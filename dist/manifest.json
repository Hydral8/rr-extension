{
  "name": "rr-extension",
  "version": "1.0",
  "description": "Extracting Data?",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://*/*",
    "http://*/*",
    "cookies"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Automatic Auth-Cookie Finder",
    "default_icon": {}
  },
  "icons": {},
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' object-src 'self'"
}
